    "name": "xarvh/elm-slides",
    "summary": "Awesome presentation framework to create your slides in Elm",
    "license": "BSD-3-Clause",
    "version": "5.0.1",
    "exposed-modules": [
        "Slides",
        "Slides.Styles",
        "Slides.SlideAnimation",
        "Slides.FragmentAnimation"
    ],
    "elm-version": "0.19.0 <= v < 0.20.0",
