        "elm/json": "1.1.2 <= v < 2.0.0",
        "elm/url": "1.0.0 <= v < 2.0.0",
        "elm-community/easing-functions": "2.0.0 <= v < 3.0.0",
        "elm-explorations/markdown": "1.0.0 <= v < 2.0.0",
        "rtfeldman/elm-css": "16.0.0 <= v < 17.0.0"
    },
    "test-dependencies": {}
